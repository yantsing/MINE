import unittest
from Helpers.Helpers import unionPartition

class TestHelpersMethods(unittest.TestCase):

    def test_unionPartition(self):
        x = [1,3,4]
        y = unionPartition(x,1)
        print y
        self.assertEqual(unionPartition(x,1), x, "unionPartion testcase 1")


if __name__ == '__main__':
    unittest.main()
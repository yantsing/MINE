from Helpers import *

class CharacteristicMatrix(object):
    '''
    classdocs
    '''


    def __init__(self, D, B, tildek):
        '''
        Constructor
        '''
        self.D = D
        self.DPerp = PerpD(D)
        self.B = B
        self.tildek
    
    def ApproxCharacteristicMatrix(self, D, B, c): 
        if B <= 3:
            print "Parameter B should be greater than 3!"
            return
        
        if c <= 0 or c > 1:
            print "Parameter B should be greater than 0 and less than 1"

    
        
     
        